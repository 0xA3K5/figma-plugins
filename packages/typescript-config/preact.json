{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Preact",
  "extends": "./base.json",
  "compilerOptions": {
    "lib": ["DOM", "ES2015", "ESNext"],
    "module": "ESNext",
    "target": "ES6",
    "jsx": "react",
    "jsxFactory": "h",
    "noEmit": true,
    "skipLibCheck": true
  }
}
